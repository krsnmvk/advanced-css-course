{
  "name": "css-and-sass-course",
  "version": "1.0.0",
  "description": "",
  "private": false,
  "scripts": {
    "watch:trillo": "sass 02-trillo/sass/main.scss 02-trillo/css/style.css -w",
    "migrator:trillo": "sass-migrator module division color --migrate-deps 02-trillo/sass/main.scss",
    "compile:trillo": "sass 02-trillo/sass/main.scss 02-trillo/css/style.compile.css",
    "prefix:trillo": "postcss 02-trillo/css/style.compile.css --use autoprefixer -o 02-trillo/css/style.prefix.css",
    "compress:trillo": "sass 02-trillo/css/style.prefix.css 02-trillo/css/style.prod.css --style=compressed",
    "build:trillo": "npm-run-all migrator:trillo compile:trillo prefix:trillo compress:trillo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krsnmvk/css-and-sass-course.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/krsnmvk/css-and-sass-course/issues"
  },
  "homepage": "https://github.com/krsnmvk/css-and-sass-course#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.92.1",
    "sass-migrator": "^2.4.2"
  }
}
